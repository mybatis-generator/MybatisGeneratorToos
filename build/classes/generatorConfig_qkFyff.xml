			connectionURL="${ecomhouserent_jdbc_url}"
			userId="${ecomhouserent_jdbc_user}"
			password="${ecomhouserent_jdbc_password}">  
        </jdbcConnection>  
        
        <javaTypeResolver>  
            <property name="forceBigDecimals" value="false"/>  
        </javaTypeResolver>  
     	
     	<!-- model配置 -->
        <javaModelGenerator targetPackage="com.qk365.fyff.entity.ecom" targetProject="${ecomhouserent_project}">  
            <property name="enableSubPackages" value="true"/>  
            <property name="trimStrings" value="true"/>  
        </javaModelGenerator>  
        
        <!-- mapping配置 -->
        <sqlMapGenerator targetPackage="com.qk365.fyff.mapping.ecom" targetProject="${ecomhouserent_project}">  
            <property name="enableSubPackages" value="true"/>  
        </sqlMapGenerator>  
         
        <!-- dao配置 -->
